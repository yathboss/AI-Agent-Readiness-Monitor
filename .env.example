# Elasticsearch connection (dev docker-compose defaults)
ES_URL=http://localhost:9200

# Optional auth (not used in dev mode). Leave blank unless needed.
ES_USER=
ES_PASS=

# Set to 0 to disable indexing (still prints JSON to stdout)
ES_ENABLED=1

# Runner defaults
MAX_DEPTH=3
MAX_STEPS=25
PAGE_TIMEOUT_S=25
ENQUEUE_CAP=40
POST_LOAD_WAIT_MS=1200
